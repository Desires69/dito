{
  "presets": [
    "@ditojs/babel-preset",
    ["@babel/preset-env", {
      "targets": {
        "node": "9"
      },
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "alias": {
        "~": ".",
        "@": "./src"
      }
    }],
    "@babel/plugin-transform-runtime",
    "dynamic-import-node-babel-7",
    "transform-promise-to-bluebird"
  ],
  "comments": false
}
