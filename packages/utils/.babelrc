{
  "presets": [
    "@ditojs/babel-preset",
    ["@babel/preset-env", {
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "alias": {
        "~": ".",
        "@": "./src"
      }
    }],
    "@babel/plugin-transform-runtime"
  ],
  "comments": false
}
