{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"],
        "node": "9"
      },
      "useBuiltIns": "usage"
    }],
    ["@babel/preset-stage-1", {
      "decoratorsLegacy": true
    }]
],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "alias": {
        "~": ".",
        "@": "./src"
      }
    }]
  ]
}
