{
  "presets": [
    "@ditojs/dito",
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 8"
        ]
      },
      "useBuiltIns": "usage",
      "modules": false
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "comments": false
}
