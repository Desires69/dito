{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": "usage"
    }],
    ["@babel/preset-stage-1", {
      "decoratorsLegacy": true
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "comments": false
}
